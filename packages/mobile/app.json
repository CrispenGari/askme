{
  "expo": {
    "name": "askme",
    "slug": "askme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-1.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/logo-1.png",
      "color": "#311b92",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Colab.notes",
      "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/logo-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-1.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/logo-1.png"
    }
  }
}
