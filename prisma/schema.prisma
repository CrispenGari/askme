
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model User {
  id       String  @unique @default(uuid())
  phoneNumber String @unique
  password String
  avatar   String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
